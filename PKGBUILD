package() {
	echo -e "\e[01;31m\n==> WARNING!!!\e[01;37m The \"${pkgname}\" package is no longer necessary.\e[00m"
	echo -e "\e[01;37mUse the \"imule\" package instead.\e[00m\n"
	echo -e "\e[01;33mAUR: \e[01;34mhttp://aur.archlinux.org/packages.php?ID=35015\e[00m"
	echo -e "\e[01;33mPKGBUILD: \e[01;34mhttp://aur.archlinux.org/packages/im/imule/PKGBUILD\n"
	return 1
}

